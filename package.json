    "react-router": "^4.0.0-alpha.3"
  },
  "peerDependencies": {
    "react": "15.x"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^15.0.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.13.0",
    "eslint": "^3.5.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-react": "^6.3.0",
    "jest": "^15.1.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-test-renderer": "^15.3.2",
    "redux": "^3.6.0"
  },
  "jest": {
    "testPathDirs": [
      "modules/"
    ]
  }
