{
  "name": "react-router-redux",
  "version": "5.0.0-alpha.1",
  "description": "Synchronize React Router with your Redux store",
  "repository": "reactjs/react-router-redux",
  "license": "MIT",
  "main": "modules/index.js",
  "author": "Tim Dorr <timdorr@timdorr.com>",
  "files": [
    "*.md",
    "*.js",
    "LICENSE"
  ],
  "scripts": {
    "build-lib": "babel ./modules -d . --ignore '__tests__'",
    "build-umd": "webpack modules/index.js umd/react-router-redux.js",
    "build-min": "webpack -p modules/index.js umd/react-router-redux.min.js",
    "clean": "rm $(ls modules | grep -v __)",
    "test": "npm run lint && jest modules",
    "start": "jest modules --watch",
    "lint": "eslint modules"
